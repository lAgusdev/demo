<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.java.tp.NewTravelController">
   
   <children>
      <Label text="Crear Viaje" />
      <Label text="Seleccione el destino del viaje:" />
      <ComboBox fx:id="opcionesComboBox" promptText="Destino"/>
      <Button fx:id="Continuar" text="Continuar View" onAction="#switchTosegundaEtapaCrearViajeLargaDis"/>
   </children>
   
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
